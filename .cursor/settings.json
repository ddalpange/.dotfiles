{
  "better-comments.highlightPlainText": true,
  "cSpell.autoFormatConfigFile": true,
  "cSpell.showSuggestionsLinkInEditorContextMenu": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "vscode.typescript-language-features",
  "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  "editor.inlineSuggest.enabled": false,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 0.6,
  "editor.parameterHints.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "errorLens.excludeBySource": ["deno-ts(2695)", "ts(1055)"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "files.associations": {
    "*.json": "jsonc"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.detectSubmodules": false,
  "git.enableSmartCommit": true,
  "githubPullRequests.pullBranch": "never",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.useAliasesForRenames": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.enableRunFromFolder": true,
  "search.exclude": {
    "**/dist": true,
    "**/es": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.splitCwd": "initial",
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.restoreWindows": "folders",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editorAssociations": {
    "*.zip": "default",
    "git-rebase-todo": "default"
  },
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": [
      ".github/workflows/*.yml",
      ".github/workflows/*.yaml"
    ]
  },
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "git.allowForcePush": true,
  "git.closeDiffOnOperation": true,
  "git.pruneOnFetch": true,
  "json.schemaDownload.enable": true,
  "playwright.reuseBrowser": true,
  "git.confirmForcePush": false,
  "playwright.showTrace": false,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.suggest.shareSuggestSelections": true,
  "window.commandCenter": false,
  "git.useEditorAsCommitInput": false,
  "window.zoomLevel": 2,
  "editor.fontSize": 15,
  "terminal.integrated.fontSize": 14,
  "workbench.list.smoothScrolling": true,
  "githubPullRequests.upstreamRemote": "never",
  "workbench.editor.customLabels.patterns": {
    "**/src/**/index.tsx": "${dirname}.${extname}",
    "**/src/**/index.ts": "${dirname}.${extname}"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__genearted__/*": true
  },
  "search.globalFindClipboard": true,
  "githubPullRequests.pushBranch": "always",
  "editor.linkedEditing": true,
  "editor.find.globalFindClipboard": true,
  "emmet.excludeLanguages": ["markdown", "typescript"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "multiDiffEditor.experimental.enabled": false,
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "editor.fontFamily": "Menlo, 'CasCadia Code', Menlo, Monaco, 'Courier New', monospace",
  "biome.enabled": false,
  "cSpell.diagnosticLevel": "Hint",
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.cloudPatches.enabled": false,
  "gitlens.ai.experimental.generateCommitMessage.enabled": false,
  "gitlens.views.commits.pullRequests.enabled": false,
  "typescript.preferences.autoImportSpecifierExcludeRegexes": [
    "^lodash/.*$",
    "^lodash-es/.*$",
    "^date-fns/.*$",
    "^date-fns/fp/.*$",
    "^ts-pattern/.*$",
    "^@sentry-core/.*$",
    "^@sentry-react/.*$",
    "assert/*"
  ],
  "editor.suggest.localityBonus": true,
  "javascript.experimental.updateImportsOnPaste": true,
  "workbench.panel.opensMaximized": "always",
  "typescript.tsserver.enableTracing": true,
  "typescript.tsserver.maxTsServerMemory": 6144,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.jsxAttributeCompletionStyle": "braces",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.log": "off",
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, tsr.config.*, vite.config.*, README.md, postcss.config.js, .eslintrc.*,  tailwind.config.*, index.html, rsbuild.config.*, playwright.config.*, vitest.config.*, .sentryclirc, .npmrc",
    "schema.graphql": "relay.config.*"
  },
  "gitlens.launchpad.indicator.enabled": false,
  "cSpell.userWords": ["clsx"],
  "editor.wordWrap": "on",
  "editor.stickyScroll.enabled": false,
  "workbench.colorTheme": "Default High Contrast"
}
